{
    "protoc" : {
        "path": "protoc",
        "compile_on_save": true,
        "options": [
            "--experimental_allow_proto3_optional",
            "--proto_path=proto/",
            "--proto_path=${workspaceRoot}/proto",
            "--python_out=${workspaceRoot}/app/src/proto",
            "--python_out=${workspaceRoot}/host/proto"
        ]
    }
}